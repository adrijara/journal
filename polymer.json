{
  "entrypoint": "index.html",
  "shell": "src/my-app.js",
  "sources": ["images/**/*", "src/**/*"],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "lint": {
    "rules": ["polymer-3"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
